{
	"name": "crossbeats REV. Score Data Extractor",
	"version": "0.0.2",
	"manifest_version": 2,
	"permissions": [
		"tabs",
		"clipboardRead"
	],
	"description": "crossbeats REV. のユーザサイトからスコアデータ一覧をクリップボードにコピーします",
	"background": {
		"page": "background.html"
	},
	"page_action": {
		"default_title": "cbrev playdata export"
	},
	"content_scripts": [
		{
			"matches": ["https://rev-www.ac.capcom.jp/playdatamusic"],
			"js": ["jquery-2.1.3.min.js", "content.js"]
		},
		{
			"matches": ["https://rev-www.ac.capcom.jp/rplist"],
			"js": ["jquery-2.1.3.min.js", "content_rp.js"]
		}
	]
}
